import tkinter as tk
from tkinter import ttk

class Application(tk.Frame):
    def __init__(self, master=None):
        super().__init__(master)

        self.master = master
        self.grid(row=0, column=0, sticky=tk.W+t.E)
        self.create_widgets()

    def create_widgets(self):
        self.file = tk.Menubutton(self, text='File')
        self.file.pack(side='top')
        self.file.menu = tk.Menu(self.file)
        self.file['menu'] = self.file.menu

        self.file.menu.add_command(label='Open .wad', command=self.open_wad)

        self.tree_view = ttk.Treeview(self)
        self.tree_view.insert('', 0, text='foo')
        self.tree_view.pack(side='left')

    def open_wad(self):
        print('opsod')

root = tk.Tk()
root.resizable(True, True)
app = Application(root)
app.mainloop()
